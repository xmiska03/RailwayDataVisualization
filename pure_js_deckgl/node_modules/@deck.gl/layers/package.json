{
  "name": "@deck.gl/layers",
  "description": "deck.gl core layers",
  "license": "MIT",
  "type": "module",
  "version": "9.0.40",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "webgl",
    "visualization",
    "layer"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/visgl/deck.gl.git"
  },
  "types": "dist/index.d.ts",
  "main": "dist/index.cjs",
