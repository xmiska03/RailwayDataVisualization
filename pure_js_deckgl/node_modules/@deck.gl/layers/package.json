    "@math.gl/polygon": "^4.0.0",
    "@math.gl/web-mercator": "^4.0.0",
    "earcut": "^2.2.4"
  },
  "peerDependencies": {
    "@deck.gl/core": "^9.0.0",
    "@loaders.gl/core": "^4.2.0",
    "@luma.gl/core": "~9.0.0",
